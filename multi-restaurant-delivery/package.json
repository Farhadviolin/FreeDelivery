{
  "name": "multi-restaurant-delivery",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"yarn dev:customer\" \"yarn dev:driver\" \"yarn dev:admin\"",
    "dev:customer": "cd apps/customer && next dev",
    "dev:driver": "cd apps/driver && next dev",
    "dev:admin": "cd apps/admin && next dev",
    "lint": "eslint --ext .ts,.tsx apps packages",
    "format": "prettier --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "check:all": "eslint \"src/**/*.{js,ts,tsx}\" --fix && prettier --write \"src/**/*.{js,ts,tsx,json,md}\" && jest --coverage && snyk test --severity-threshold=high"
  },
  "devDependencies": {
